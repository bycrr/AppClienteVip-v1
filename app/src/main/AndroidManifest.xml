<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="br.com.bycrr.v4.appclientevip">
  <uses-permission android:name="android.permission.INTERNET" />

  <application
    android:allowBackup="true"
    android:largeHeap="true"
    android:usesCleartextTraffic="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="br.com.bycrr.v4.appclientevip.view.ConsultarClientesActivity"/>
    <activity android:name="br.com.bycrr.v4.appclientevip.view.RecuperarSenhaActivity" />
    <activity android:name="br.com.bycrr.v4.appclientevip.view.ClientePessoaJuridicaActivity" />
    <activity android:name="br.com.bycrr.v4.appclientevip.view.ClientePessoaFisicaActivity" />
    <activity android:name="br.com.bycrr.v4.appclientevip.view.ClienteVipActivity" />
    <activity android:name="br.com.bycrr.v4.appclientevip.view.LoginActivity" />
    <activity android:name="br.com.bycrr.v4.appclientevip.view.MainActivity" />
    <activity android:name="br.com.bycrr.v4.appclientevip.view.CredencialAcessoActivity" />
    <activity android:name="br.com.bycrr.v4.appclientevip.view.SplashActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>
