<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="br.com.bycrr.v5.appclientevip">

  <uses-permission android:name="android.permission.INTERNET" />

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:largeHeap="true"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme"
    android:usesCleartextTraffic="true">
    <activity android:name=".view.MeusDadosActivity"></activity>
    <activity android:name=".view.ConsultarClientesActivity" />
    <activity android:name=".view.RecuperarSenhaActivity" />
    <activity android:name=".view.ClientePessoaJuridicaActivity" />
    <activity android:name=".view.ClientePessoaFisicaActivity" />
    <activity android:name=".view.ClienteVipActivity" />
    <activity android:name=".view.LoginActivity" />
    <activity android:name=".view.MainActivity" />
    <activity android:name=".view.CredencialAcessoActivity" />
    <activity android:name=".view.SplashActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>
